{
    "app-id": "org.gnome.Calculator",
    "runtime": "org.gnome.Platform",
    "runtime-version": "42",
    "sdk": "org.gnome.Sdk",
    "command": "gnome-calculator",
    "finish-args": [
        "--device=dri",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--share=network",
        "--metadata=X-DConf=migrate-path=/org/gnome/calculator/"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/share/pkgconfig",
        "/share/aclocal",
        "/man",
        "/share/man",
        "/share/gtk-doc",
        "/share/vala",
        "*.la",
        "*.a",
        "/bin/gcalccmd",
        "/lib/girepository-1.0",
        "/share/info",
        "/share/gtksourceview-5",
        "/share/doc",
        "/share/gir-1.0"
    ],
    "modules": [
        {
            "name": "mpfr",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.gnu.org/gnu/mpfr/mpfr-4.1.0.tar.xz",
                    "sha256": "0c98a3f1732ff6ca4ea690552079da9c597872d30e96ec28414ee23c95558a7f"
                }
            ]
        },
        {
            "name": "mpc",
            "config-opts": [
                "--with-mpfr=/app"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://ftp.gnu.org/gnu/mpc/mpc-1.2.1.tar.gz",
                    "sha256": "17503d2c395dfcf106b622dc142683c1199431d095367c6aacba6eec30340459"
                }
            ]
        },
        {
            "name": "gnome-calculator",
            "buildsystem": "meson",
            "config-opts": [
                "-Ddisable-introspection=true"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnome-calculator/42/gnome-calculator-42.1.tar.xz",
                    "sha256": "ef4d24e42a65dc860e6206f3b470b7d233c2ae2a4d4965d8859aa9ea8a395acd",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "gnome-calculator"
                    }
                }
            ]
        }
    ]
}
